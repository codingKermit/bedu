<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.care.bedu.bookmark.dao.BookmarkDAO">

    <select id="inOut" parameterType="com.care.bedu.bookmark.vo.BookmarkVO">
        CALL B_EDU.BEDU_BOOKMARK_INOUT_PRO(#{userName}, #{lectNum})
    </select>

    <select id="getList" resultType="com.care.bedu.bookmark.vo.BookmarkVO">
        SELECT
            bookmark_num AS num,
            user_name as userName,
            lect_num as lectNum,
            bookmark_date as bookmarkDate
        FROM
            B_EDU.t_bedu_bookmark
        WHERE
            user_name = #{userName}
    </select>


</mapper>